pipeline {
    agent any
        stages {
            stage('Init') {
                steps {
                    echo 'this is the init step!'
                }
            }
            stage('Build') {
                steps {
                    echo 'Build the app!'
                }
            }
            stage('Deploy') {
                steps {
                    echo 'Code deploy!'
                }
            }
        }
    }
